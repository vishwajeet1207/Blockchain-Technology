pragma solidity 0.8;
contract Bank
{
address public owner;
uint public balance;
constructor()
{
owner = msg.sender;
}
function deposit() external payable
{
balance += msg.value;
}
function withdraw(uint amount) external
{
require(balance >= amount, "Insufficient balance!");
payable(owner).transfer(amount);
balance -= amount;
}
}
